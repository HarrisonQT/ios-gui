{
  "name": "ios-gui",
  "version": "1.0.0",
  "description": "Visual iOS GUI component for displaying content within a iOS device on a webpage",
  "main": "pages/index.js",
  "scripts": {
    "dev": "next -- -p 1111",
    "build": "next build",
    "start": "next start",
    "lint": "eslint ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "next": "^9.1.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-iframe": "^1.8.0",
    "react-is": "^16.12.0",
    "styled-components": "^5.0.0-rc.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisonQT/ios-gui"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.6"
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
